<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>util.login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DButil</servlet-name>
        <servlet-class>DB.DButil</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>register</servlet-name>
        <servlet-class>util.register</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>adminlogin</servlet-name>
        <servlet-class>util.adminlogin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>OrderServlet</servlet-name>
        <servlet-class>IO.OrderServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>adminmodifyproduct</servlet-name>
        <servlet-class>IO.adminmodifyproduct</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>admindeleteproduct</servlet-name>
        <servlet-class>IO.admindeleteproduct</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>adminaddproduct</servlet-name>
        <servlet-class>IO.adminaddproduct</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Checkout</servlet-name>
        <servlet-class>util.Checkout</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteOrderItem</servlet-name>
        <servlet-class>IO.DeleteOrderItem</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>updateorder</servlet-name>
        <servlet-class>IO.updateorder</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DButil</servlet-name>
        <url-pattern>/DButil</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>register</servlet-name>
        <url-pattern>/register</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>adminlogin</servlet-name>
        <url-pattern>/adminlogin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>OrderServlet</servlet-name>
        <url-pattern>/order</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>adminmodifyproduct</servlet-name>
        <url-pattern>/adminmodifyproduct</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>admindeleteproduct</servlet-name>
        <url-pattern>/admindeleteproduct</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>adminaddproduct</servlet-name>
        <url-pattern>/adminaddproduct</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Checkout</servlet-name>
        <url-pattern>/Checkout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteOrderItem</servlet-name>
        <url-pattern>/DeleteOrderItem</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>updateorder</servlet-name>
        <url-pattern>/updateorder</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
